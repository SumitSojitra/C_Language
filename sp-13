           5
         4 5 4
       3 4 5 4 3
     3 3 4 5 4 3 2
   1 2 3 4 5 4 3 2 1

#include<stdio.h>

void main()
{
	for(int i=5; i>=1; i--)
	{
		for(int s=1; s<i; s++)
		{
			printf(" ");
		}
		for(int j=i; j<=5; j++)
		{
			printf("%d", j);
		}
		for(int j=5; j>=i; j--)
		{
			if(j!=5)
			{
				printf("%d", j);
			}
		}
		printf("\n");
	}	
}
